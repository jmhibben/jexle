{
  "name": "template-engine",
  "version": "0.0.1",
  "description": "Template engine for Markadillo, built as a standalone manager",
  "main": "app.js",
  "author": "James Hibben <jmhibben@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"parcel watch index.html\" \"npm run serve\"",
    "build": "",
    "serve": "node server/server",
    "test": "ava",
    "test:w": "ava --watch"
  },
  "ava": {
    "files": [
      "test/**/*.spec.ts",
      "!node_modules"
    ],
    "sources": [
      "src/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "tap": true,
    "verbose": true
  },
  "dependencies": {
    "@hapi/hapi": "^18.3.1",
    "markdown-it": "^9.0.1",
    "mobx": "^5.10.1",
    "mobx-preact": "^3.0.0",
    "preact": "^8.4.2",
    "preact-material-components": "^1.5.8",
    "preact-routlet": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "concurrently": "^4.1.1",
    "lodash": "^4.17.14",
    "lodash-es": "^4.17.14",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.21.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
